{
  "name": "react-d3-cloud",
  "version": "0.1.1",
  "description": "a word cloud component using d3-cloud",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "NODE_ENV=production npm run build:dev",
    "build:dev": "babel src -d lib",
    "test-only": "NODE_ENV=test mocha $npm_package_options_mocha",
    "test:watch": "npm run test-only -- --watch",
    "coverage": "nyc --check-coverage --lines 10 --functions 10 npm run test-only",
    "test": "npm run lint && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoctol/react-d3-cloud.git"
  },
  "keywords": [
    "word-cloud",
    "react",
    "d3"
  ],
  "author": "cph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yoctol/react-d3-cloud/issues"
  },
  "homepage": "https://github.com/Yoctol/react-d3-cloud#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "canvas": "^1.4.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "enzyme": "^2.4.1",
    "eslint": "3.9.1",
    "eslint-config-yoctol": "^0.9.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "nyc": "^8.4.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "options": {
    "mocha": "--compilers js:babel-register --require ./test/setup.js --recursive"
  },
  "dependencies": {
    "d3": "^4.2.1",
    "d3-cloud": "^1.2.1",
    "react-faux-dom": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0"
  }
}
