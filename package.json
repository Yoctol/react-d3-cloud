{
  "name": "react-d3-cloud",
  "version": "0.6.0",
  "description": "a word cloud component using d3-cloud",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "NODE_ENV=production npm run build:dev",
    "build:dev": "babel src -d lib --ignore __tests__",
    "testonly": "jest --no-cache",
    "testonly:watch": "npm run testonly -- --watch",
    "test": "npm run lint && npm run testonly",
    "preversion": "npm test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoctol/react-d3-cloud.git"
  },
  "keywords": [
    "word-cloud",
    "react",
    "d3"
  ],
  "author": "cph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yoctol/react-d3-cloud/issues"
  },
  "homepage": "https://github.com/Yoctol/react-d3-cloud#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "canvas": "^1.6.13",
    "enzyme": "^3.7.0",
    "eslint": "^5.7.0",
    "eslint-config-yoctol": "^0.18.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2"
  },
  "dependencies": {
    "d3-cloud": "^1.2.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.3.2",
    "enzyme-adapter-react-16": "^1.6.0",
    "react-faux-dom": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./test/setupJest.js"
    ],
    "timers": "fake",
    "resetModules": true,
    "resetMocks": true
  }
}
